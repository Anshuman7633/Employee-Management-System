   <string notr="true">/* Style the main dialog */
QDialog {
    background-color: #2E3440; /* Dark grayish-blue background */
    color: #ECEFF4; /* Light text for better contrast */
    font-family: &quot;Arial&quot;, sans-serif;
    font-size: 14px;
    border: 2px solid #4C566A;
    border-radius: 10px;
    padding: 10px;
}

/* Style the QGroupBox */
QGroupBox {
    border: 2px solid #88C0D0; /* Soft cyan border */
    border-radius: 8px;
    margin-top: 10px;
    color: #ECEFF4; /* Light text */
    font-weight: bold;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 5px 15px;
    background-color: #3B4252; /* Slightly darker background */
    color: #ECEFF4;
    border-radius: 5px;
}

/* Style QLabel */
QLabel {
    font-size: 20px;
    color: #D8DEE9; /* Light text color */
}

/* Style QLineEdit */
QLineEdit {
    background-color: #3B4252; /* Dark input field */
    color: #ECEFF4; /* Light text */
    border: 2px solid #4C566A; /* Subtle border */
    border-radius: 5px;
    padding: 5px;
    font-size: 14px;
}

QLineEdit:focus {
    border: 2px solid #88C0D0; /* Cyan border on focus */
}

/* Style QPushButton */
QPushButton {
    background-color: #5E81AC; /* Soft blue button */
    color: #ECEFF4; /* Light text */
    border: 2px solid #4C566A; /* Subtle border */
    border-radius: 8px;
    padding: 5px 15px;
    font-size: 14px;
    font-weight: bold;
}

QPushButton:hover {
    background-color: #81A1C1; /* Lighter blue on hover */
    border: 2px solid #88C0D0; /* Cyan border */
}

QPushButton:pressed {
    background-color: #4C566A; /* Darker blue on click */
    border: 2px solid #3B4252; /* Darker border */
}

QPushButton:disabled {
    background-color: #3B4252; /* Grayish background for disabled button */
    color: #4C566A; /* Dimmed text */
    border: 2px solid #3B4252;
}
</string>
